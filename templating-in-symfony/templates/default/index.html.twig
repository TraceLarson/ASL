{% extends 'base.html.twig' %}

{% block title %}Home{% endblock %}

{% block header %}
    {{ parent() }}
    <h2>Child Header</h2>
    {% if loggedin %}
        Welcome!
    {% else %}
        <a href="/login">Login</a>
    {% endif %}
{% endblock %}

{% block body %}
    <p>This is body text</p>
    <ul>
        {% for user in users %}
            <li>{{ user }}</li>
        {% endfor %}
    </ul>
    <ol>
        {% for user in users %}
            {{ include('parts/user.html.twig', {'name': user}) }}
        {% endfor %}
    </ol>
{% endblock %}